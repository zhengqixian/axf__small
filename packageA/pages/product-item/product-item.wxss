.main{
  top:0;
  bottom: 100rpx;
}
.main image{
  margin:7%;
}
.wraper{
  background-color: #f8f8f8;
  font-size: 28rpx;
}
.product-item-block{
  margin-bottom: 10rpx;
  background-color: #fff;
}
.bannerContainer{
  width: 100%;
}
.product-name{
  padding: 0 20rpx;
  color: #292d33;
  font-size: 46rpx;
  line-height: 60rpx;
  text-align: center;
  white-space: normal;
}
.product-shopping{
  padding: 20rpx 0 60rpx 0;
  text-align: center;
  color: #f40;
  font-size: 36rpx;
  font-weight: 400;
  line-height: 58rpx;
}
.product-shopping text{
    font-size: 44rpx;
    font-weight: 700;
  }
.product-properties{
  padding: 30rpx;
}
.title{
  margin-bottom: 30rpx;
  background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAV8AAAAEBAMAAADbpSEyAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAqUExURQAAAOC9auC9auC9auC9auC9auC9auC9auC9auC9auC9auC9auC9auC9agdMhdkAAAANdFJOUwCZRGaID/C0S1EkGCCdfVLsAAAAM0lEQVQoz2NgIAWw3r3GQFXQe7eAgZZgMDjYhVhwgHYO5hQkGggwGBMLNtDOwVxKRAMFAOUAK2mKnCc7AAAAAElFTkSuQmCC') center center no-repeat;
  -webkit-background-size: auto 4rpx;
  background-size: auto 4rpx;
  text-align: center;
  color: #e0bd6a;
}
.product-properties{
  background-color: #fff;
  margin-bottom: 10rpx;
}

.property-item{
  line-height: 60rpx;
}
.leading-word{
  display: inline-block;
  color: #848c99;
}
.content-word{
  padding-left: 100rpx;
  color: #333;
}
.product-properties view{
    line-height: 1.5;
    color: #e53333;
    font-size: 28rpx;
}
.subimg image{
  width: 100%;
  margin: 0;
  padding-bottom: 90rpx;
}
.favor{
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
  background: url("data:image/png;base64,") top center no-repeat;
  width:80rpx;
  line-height: 36rpx;
  padding-top: 36rpx;
  background-size: 36rpx auto;
  font-size: 20rpx;
  text-align: center;
}
.favor.selectd{
  background: url('https://s1.ax1x.com/2018/02/05/9KBO41.png') 8rpx 20rpx no-repeat;
  background-size: 36rpx auto;}
.operates{
  padding-left: 100rpx;
  font-size: 28rpx;
}
.product-operates{
  float: right;
  width: 50%;
  overflow: hidden;
  position: absolute;
  left: 240rpx;
  top: 50%;
  line-height: 0;
  transform: translateY(-50%);
}
.product-operates>.inner{
  display: inline-block;
  font-size: 48rpx;
  width: 58rpx;
  height: 58rpx;
  line-height: 68rpx;
  color: #d73e00;
  text-align: center;
  border: 2rpx solid #dcbd9e;
  border-radius: 50%;
}
.product-operates-item{
  display: inline-block;
  height: 58rpx;
  line-height: 58rpx;
  vertical-align: top;
  font-size: 32rpx;
  margin: 0 10rpx;
}
.inner.product-operates-sub{
  line-height: 36rpx;
  font-size: 88rpx;
}
.inner.product-operates-add{
  font-size: 60rpx;
  vertical-align: top;
}
.cart-btn{
  position: absolute;
  width: 108rpx;
  height: 108rpx;
  background:#ffd600 url('https://s1.ax1x.com/2018/02/05/9KD15n.png') center center no-repeat;
    background-size: 80%;
  z-index: 3;
  right: 12rpx;
  bottom: 14rpx;
  border-radius: 50%;
  border: 6rpx solid #f7f7b2;
}
.cart-btn-num{
  position: absolute;
  right: 0;
  top: 0;
  border-radius: 50%;
  background-color: #f40;
  color: #ffffff;
  line-height: 36rpx;
  width: 36rpx;
  text-align: center;
  font-size: 20rpx;
  -webkit-transform: scale(1);
  -ms-transform: scale(1);
  -o-transform: scale(1);
  transform: scale(1);
  -webkit-transition: all 0.3s cubic-bezier(0.28,-0.42, 0.37, 1.55);
  -o-transition: all 0.3s cubic-bezier(0.28,-0.42, 0.37, 1.55);
  transition: all 0.3s cubic-bezier(0.28,-0.42, 0.37, 1.55);
}
.cart-btn-num.active{
  -webkit-transform: scale(1.5);
  -ms-transform: scale(1.5);
  -o-transform: scale(1.5);
  transform: scale(1.5);
}
.footer{
    height: 100rpx;
    line-height: 100rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    background-color:white;
    font-size: 28rpx;
    border-top: 1px solid #e0e0e0;
    z-index: 100;
  }
  .collect-span{
  position: absolute;
  left: 16rpx;
  padding-top: 20rpx;
  font-size: 24rpx;
  background: url('https://s1.ax1x.com/2018/02/05/9KBHHJ.png') 8rpx 20rpx no-repeat;
  background-size: 36rpx auto;
}
.collect-span.active{
  background: url('https://s1.ax1x.com/2018/02/05/9KBO41.png') 8rpx 20rpx no-repeat;
  background-size: 36rpx auto;
}
.collect-add{
  text-align: left;
  margin-left: 100rpx;
}
.btn-wrap{
  position: absolute;
  left: 260rpx;
  bottom: 0;
  width: 180rpx;
  height:100rxp;
  line-height: 80rpx;
}
.btn-wrap text:nth-child(2n+1){
    display: inline-block;
    width: 50rpx;
    height: 50rpx;
    line-height: 43rpx;
    text-align: center;
    border: 1px solid #f40;
    color: #f40;
    border-radius: 50%;
    font-size: 48rpx;
  }
.btn-wrap text:nth-child(2){
    display: inline-block;
    width: 40rpx;
    height:60rpx;
    position: relative;
    top:-6rpx;
    text-align: center;
    padding: 0 10rpx;
  }
  .append{
  position: absolute;
  top: -40rpx;
  right: 30rpx;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border:.1rem solid #e0e0e0;
  text-align: center;
  z-index: 1;
  }
  .append-img{
    width: 108rpx;
    height: 108rpx;
    background:#ffd600 url('https://s1.ax1x.com/2018/02/05/9KD15n.png') center center no-repeat;
    background-size: 80%;
    border-radius: 50%;
    margin: 0;
    border: 6rpx solid #f7f7f7;
  }
   .append text{
    position: absolute;
    top: 0;
    right: 0;
    width: 36rpx;
    height: 36rpx;
    line-height: 36rpx;
    font-size:20rpx;
    background-color: #ff4400;
    border-radius: 50%;
    color:white;
  }