.main{
  top: 0;
}
.empty-notice{
  padding-top: 20%;
  text-align: center;
  line-height: 80rpx;
  font-size: 28rpx;
}
.notice-icon{
  width: 160rpx;
  height: 160rpx;
  border-radius: 80rpx;
  background: url('data:image/png;base64,') center center no-repeat;
  background-size: contain;
  margin: 0 auto;
}
.theme-spline{
  border: 1px solid #e0e0e0;
  padding: 8rpx 60rpx;
  border-radius:6rpx;
}
.page-cart{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0rpx;
}
.main{
  bottom: 0;
}
.receive{
  font-size: 28rpx;
  font-weight: 400;
  width: 100%;
  position: relative;
  background-image: url("data:image/png;base64,"),url("data:image/png;base64,");
  background-position: left top,left bottom;
  background-repeat: repeat-x;
  -webkit-background-size: auto 6rpx;
  background-size: auto 6rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}
.receive .tr{
  display: flex;
}
.receive .th{
  padding: 14rpx 20rpx 14rpx 30rpx;
  text-align: left;
  width: 28%;
  box-sizing: border-box;
}
.receive .more{
  position: absolute;
  right: 0;
  top: 50%;
  transform: translate(0, -50%);
}
.receive .td{
  flex: 1;
  padding: 14rpx 0;
}
.cart-group{
  padding-top: 10rpx;
  font-size: 24rpx;
  background-color: #fff;
}
.cart-group .group-name{
  padding-left: 28rpx;
  overflow: hidden;
  padding-right: 24rpx;
}
.cart-group .group-name>text:nth-child(1){
  float: left;
  color: #999;
}
.cart-group .group-name>text:nth-child(2){
  float: right;
  height: 48rpx;
  line-height: 48rpx;
  border: 1px solid #ff3800;
  border-radius: 24rpx;
  width: 140rpx;
  text-align: center;
  color: #e64f1a;
}
.cart-group .group-name>text:nth-child(1):before{
  content: '';
  display: inline-block;
  width: 8rpx;
  height: 24rpx;
  border-radius: 4rpx;
  margin-right: 10rpx;
  background-color: #ffd600;
  vertical-align: -4rpx;
}
.group-proptext{
  color: #999;
  padding-left: 28rpx;
  height: 40rpx;
  line-height: 40rpx;
}
.group-receive{
  padding-left: 28rpx;
  font-size: 28rpx;
  height: 100rpx;
  line-height: 100rpx;
}
.group-receive-ext{
  float: right;
}
.group-receive-select{
  color: #e64f1a;
  outline: none;
  border: none;
  font-size: 28rpx;
  background-color: transparent;
  display: inline-block;
}
.group-comment{
  height: 100rpx;
  line-height: 100rpx;
  display: -webkit-flex;
  display: flex;
}
.group-comment-txt{
  float: left;
}
.group-comment-input-wrap{
  flex:1;
  display: flex;
  align-items: center;
  padding-right: 44rpx;
}
.group-comment-input{
  height:64rpx;
  border: 1px solid #cecece;
  border-radius: 10rpx;
  width: 100%;
  box-sizing: border-box;
  outline: none;
  text-indent: 10rpx;
  font-size: 28rpx;
  padding-left: 8rpx;
}
.group-item-checkbox{
  background: url("data:image/png;base64,") center center no-repeat;
  -webkit-background-size: 36rpx 36rpx;
  background-size: 36rpx 36rpx;
  width: 100rpx;
}
.group-item-checkbox.active{
  background-image: url("data:image/png;base64,");
}
.group-item-img{
  width: 100rpx;
}
.group-item-img>image{
  width: 100rpx;
  height: 100rpx;
}
.product-operates{
  text-align: right;
}
.group-item-detail{
  padding-right: 24rpx;
  width: 550rpx;
}
.group-item-detail>view{
  height: 80rpx;
  line-height: 80rpx;
}
.group-footer{
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
}
.selectAll{
  display: inline-block;
  width: 100rpx;
  padding-left: 50rpx;
  -webkit-background-size: 36rpx 36rpx;
  background-size: 36rpx 36rpx;
  background-repeat: no-repeat;
  background-position: left center;
}
.selectAll-false{
  background-image: url("data:image/png;base64,");
}
.selectAll-true{
  background-image: url('data:image/png;base64,');
}
.group-btn{
  height: 100%;
  float: right;
  width: 200rpx;
  text-align: center;
}
.product-specifics-wrap{
  overflow: hidden;
}
.product-specifics{
  float: left;
  width: 30%;
  overflow: hidden;
}
.product-specifics>text{
  display: block;
  color: #858585;
  margin-bottom: 20rpx;
}
.product-specifics>var{
  color: #f40;
}
.product-operates{
  float: right;
  width: 70%;
  overflow: hidden;
}
.product-operates>.inner{
  display: inline-block;
  font-size: 48rpx;
  width: 58rpx;
  height: 58rpx;
  line-height: 58rpx;
  color: #d73e00;
  text-align: center;
  border: 1px solid #dcbd9e;
  border-radius: 50%;
}
.product-operates-item{
  display: inline-block;
  height: 56rpx;
  line-height: 56rpx;
  vertical-align: top;
  font-size: 24rpx;
  margin: 0 10rpx;
}
.table{
  width: 100%;
}
.tr{
  display: table-row;
}
.td{
   display: table-cell;
   vertical-align: middle;
}
.cart-empty-btn{
  display: inline-block;
}